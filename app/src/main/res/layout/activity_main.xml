<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="1320dp"
        android:layout_height="1380dp"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/launchericon" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="392dp"
        android:layout_height="131dp"
        android:layout_marginBottom="340dp"
        android:fontFamily="@font/zalando_sans_semi_expanded_black"
        android:gravity="center"
        android:text="click to begin the ultimate to-do."
        android:textSize="30sp"
        app:fontFamily="@font/zalando_sans_semi_expanded_black"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="562dp"
        android:layout_height="147dp"
        android:fontFamily="@font/zalando_sans_semi_expanded_black"
        android:gravity="center"
        android:text="H I T L I S T"
        android:textSize="65sp"
        app:fontFamily="@font/zalando_sans_semi_expanded_black"
        android:shadowColor="#80000000"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageButton5"
        app:layout_constraintVertical_bias="0.275" />


    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/imageView"
        android:layout_width="419dp"
        android:layout_height="348dp"
        android:layout_marginBottom="292dp"
        android:src="@drawable/blood"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>